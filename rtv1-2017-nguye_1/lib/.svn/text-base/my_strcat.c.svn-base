char	*my_strcat(char *dest, char *src)
{
  int i;
  int c;

  i = 0;
  c = 0;
  while (dest[i] != 0)
    {
      i += 1;
    }
  while (src[c] != 0)
    {
      dest[i] = src[c];
      i += 1;
      c += 1;
    }
  return (dest);
}
